#ifndef FILAGRAFO_H_INCLUDED
#define FILAGRAFO_H_INCLUDED

typedef struct node{
int dado;
struct node *prox;
}Node;

typedef struct fila{
Node *primeiro;
Node *ultimo;
int size;
}Fila;

Fila* cria_fila(int n);
void insere_fila(Fila *f,int n);
void mostra_fila(Fila *f);
int remove_fila(Fila *f);


#endif // FILAGRAFO_H_INCLUDED
